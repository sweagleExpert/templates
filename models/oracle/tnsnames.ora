  # SOURCE_ROUTE possible values on | off | yes | no
  (DESCRIPTION=
   (ADDRESS= (PROTOCOL = IPC)(KEY = EXTPROC1))
   (CONNECT_DATA=
    (SERVICE_NAME=generic)
