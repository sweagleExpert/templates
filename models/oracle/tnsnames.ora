    (GLOBAL_NAME=alpha5))
   (TYPE_OF_SERVICE=rdb_database))
  (DESCRIPTION=
   (SDU=8192)
   (SEND_BUF_SIZE=11784)
   (RECV_BUF_SIZE=11784)
   (ENABLE=broken)
   (SOURCE_ROUTE=off)
   (ADDRESS_LIST=
    (SOURCE_ROUTE=off)
    (LOAD_BALANCE=off)
